<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prashant_tripathi.onlinevotingapp" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterUser"/>
        <activity android:name=".RegisterAdmin"/>
        <activity android:name=".AdminAddElection"/>
        <activity android:name=".AdminEditElection"/>
        <activity android:name=".AdminHome"/>
        <activity android:name=".LoginAdmin"/>
        <activity android:name=".AdminRemoveElection"/>
        <activity android:name=".AdminAddCandidate"/>
        <activity android:name=".AdminRemoveCandidate"/>
        <activity android:name=".AdminEditCandidate"/>
        <activity android:name=".AdminShowElections"/>
        <activity android:name=".AdminShowResults"/>
        <activity android:name=".AdminViewElections"/>
    </application>

</manifest>
